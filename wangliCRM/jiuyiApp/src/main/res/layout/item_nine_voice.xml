<?xml version="1.0" encoding="utf-8"?>
<customer.view.SquareLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:gravity="center">
    <RelativeLayout
        android:id="@+id/rl_grida_image"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="5dp"
        android:layout_gravity="center"
        android:background="@color/jiuyi_backgroud_color">
        <ImageView
            android:id="@+id/iv_item_nine_photo_photo"
            android:layout_width="15dp"
            android:layout_height="20dp"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:background="@drawable/infomation_voice"
            android:scaleType="fitXY"></ImageView>

        <TextView
            android:id="@+id/tv_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/iv_item_nine_photo_photo"
            android:layout_gravity="center_horizontal|center_vertical"
            android:gravity="center"
            android:text=""
            android:textColor="@color/jiuyi_blue"
            />

    </RelativeLayout>


</customer.view.SquareLayout>